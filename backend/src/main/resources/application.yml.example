# 数据库配置示例 (请在本地复制此文件并重命名为 application.properties，填入真实密码)
spring.datasource.url=jdbc:mysql://localhost:3306/resonance?useUnicode=true&characterEncoding=utf-8
spring.datasource.username=root
spring.datasource.password=YOUR_PASSWORD_HERE

# 端口配置
server.port=8080


# ===============================
# Redis 缓存配置
# ===============================
# Redis 服务器所在的主机IP（本地开发就是 127.0.0.1）
spring.data.redis.host=127.0.0.1
# Redis 的默认端口号是 6379
spring.data.redis.port=6379
# Redis 的密码（如果没有设置密码，就留空）
spring.data.redis.password=
# 选择使用 Redis 的几号数据库（Redis 默认自带 16 个数据库，编号 0-15，我们用 0 号）
spring.data.redis.database=0